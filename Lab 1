// Lab 1 -Vectors
#include <iostream>
#include <string>
#include <vector>
using namespace std;

int main()
{
  
  vector <char> vowels{'a','e','i','o','u'};
  cout << vowels[0]<<endl;
  cout << vowels[4]<<endl;
  
  vector <int> test_scores{12,22,32,11,98,88};
  
  cout<<test_scores.at(3)<<endl;
  cout<<test_scores.at(5)<<endl;
  
  cout<<test_scores.size()<<endl;
  
  cout<<"Enter a variable to enter into the test score: ";
  int add_score{0};
  cin>>add_score;
  
  test_scores.push_back(add_score);
  cout<<test_scores.size()<<endl;
  cout<<test_scores.capacity()<<endl;
  
  for( int a = 0; a < test_scores.size(); a = a + 1 ) {
      cout << test_scores[a]<< endl;
   }
  
  
}
